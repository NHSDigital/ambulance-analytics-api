		{
			"name": "Missing KID",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "f777fe78-32dc-40fe-bea1-aca380624db0",
						"exec": [
							"var navigator = {}; //fake a navigator object for the lib",
							"var window = {}; //fake a window object for the lib",
							"eval(postman.getGlobalVariable(\"jsrsasign-js\")); //import jsrsasign lib",
							"",
							"var header = {",
							"    \"typ\": \"JWT\",",
							"    \"alg\": \"RS256\",",
							"}",
							"",
							"var appId = postman.getEnvironmentVariable(\"app_id\");",
							"var data = {",
							"  \"iss\": appId,",
							"  \"sub\": appId",
							"};",
							"",
							"var secret = postman.getEnvironmentVariable(\"PRIVATE_KEY\");",
							"var sHeader = JSON.stringify(header);",
							"var sPayload = JSON.stringify(data);",
							"var sJWT = KJUR.jws.JWS.sign(\"RS256\", sHeader, sPayload, secret);",
							"postman.setEnvironmentVariable(\"jwt_token\", sJWT);"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "87c9b610-dc23-48ef-a35d-080f98fda0bd",
						"exec": [
							"console.log(responseBody);",
							"",
							"pm.test(\"Response Code\", function(){ ",
							"    pm.response.to.have.status(400); ",
							"    pm.response.to.have.body(\"kid claim is required to validate JWT\"); ",
							"});"

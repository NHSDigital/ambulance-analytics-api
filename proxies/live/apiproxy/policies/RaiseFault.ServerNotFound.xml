<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault async="false" continueOnError="false" enabled="true" name="RaiseFault.ServerNotFound">
  <FaultResponse>
    <Set>
      <Payload contentType="application/json">{"error": "Server Not Found"}</Payload>
      <StatusCode>404</StatusCode>
      <ReasonPhrase>Server Not Found</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault name="RaiseFault.MethodNotAllowed">
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <FaultResponse>
        <Set>
            <StatusCode>405</StatusCode>
            <ReasonPhrase>Method not allowed</ReasonPhrase>
            <Payload contentType="application/json">{"error": "Request method must be POST, not {request.verb}"}</Payload>
        </Set>
    </FaultResponse>
</RaiseFault>

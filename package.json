{
  "name": "apim-ambulance-analytics-api",
  "version": "0.0.1",
  "description": "OAS (Swagger v3) API Definition for Ambulance Analytics API",
  "scripts": {
    "lint": "node_modules/.bin/@openapitools/openapi-generator-cli validate -i specification/ambulance-analytics.yaml",
    "test": "node_modules/.bin/newman run tests/e2e/ambulance-analytics-sandbox.json -e tests/e2e/environments/local.postman.json --reporters html,cli,htmlextra --reporter-htmlextra-logs --reporter-htmlextra-darkTheme",
    "publish": "./node_modules/.bin/@openapitools/openapi-generator-cli generate -i specification/ambulance-analytics.yaml --generator-key v3.0 -g openapi -o build && rm openapitools.json && mv build/openapi.json build/ambulance-analytics.json",
    "check-licenses": "node_modules/.bin/license-checker --failOn GPL --failOn LGPL"
  },
  "author": "NHS Digital",
  "license": "MIT",
  "homepage": "https://github.com/NHSDigital/ambulance-analytics-api",
  "devDependencies": {
    "apigeetool": "^0.14.2",
    "license-checker": "^25.0.1",
    "minimist": "^1.2.2",
    "newman": "^4.6.1",
    "newman-reporter-html": "^1.0.5",
    "newman-reporter-htmlextra": "^1.22.2"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1"
  }
}
